                        <span>
                            <i class="fa fa-sort"></i>
                            <i class="fa fa-sort-asc"></i>
                            <i class="fa fa-sort-desc"></i>
                            <tal:block i18n:translate="">Coefficient</tal:block>
                        </span>
                    </th>

            <tal:block condition="not:isExam">
